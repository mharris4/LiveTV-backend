<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2" assembly = "LiveTV"
   namespace = "LiveTV.Entities">

  <class name = "Channels"  table="Channels">

    <id name = "ChannelID">
      <generator class = "native"/>
    </id>

    <property name = "SDName" column = "SDName" type = "String"/>
    <property name = "DisplayName" column = "DisplayName" type = "String"/>

  </class>

</hibernate-mapping>